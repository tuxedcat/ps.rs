	"read vector": {
		"scope": "rust",
		"prefix": "__readv",
		"body": [
			"fn readv<T>()->Vec<T> where T:FromStr{",
			"\tlet mut s = String::new();",
			"\tio::stdin().read_line(&mut s).unwrap();",
			"\ts.split_whitespace().flat_map(str::parse::<T>).collect()",
			"}",
		],
